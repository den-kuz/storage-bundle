{
  "name": "d3n/storage-bundle",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "D3N\\StorageBundle\\": "src/"
    }
  },
  "require": {
    "php": ">=8.2",
    "symfony/config": "^7",
    "symfony/dependency-injection": "^7",
    "symfony/filesystem": "^7",
    "symfony/finder": "^7",
    "symfony/http-kernel": "^7"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "phpcscheck": [
      "php-cs-fixer fix --dry-run"
    ],
    "phpcsfix": [
      "php-cs-fixer fix"
    ]
  }
}
